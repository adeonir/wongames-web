{
  "name": "won-games-web",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "serve": "next start",
    "export": "next build && next export",
    "lint": "eslint src --max-warnings=0",
    "pretty": "yarn prettier src --write",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --maxWorkers=50% --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node src/utils/graphql/convertToNonNull.js",
    "codegen:generate": "apollo client:codegen --target=typescript --no-addTypename --excludes=node_modules --includes=src/graphql/queries/*,src/graphql/fragments/* --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/types",
    "codegen:lint": "prettier --write src/graphql/types/**/*.ts",
    "codegen": "yarn schema:download && yarn codegen:generate && yarn codegen:lint",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook -s ./public",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test --findRelatedTests --bail"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn eslint --fix",
      "yarn prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.5.6",
    "@styled-icons/boxicons-regular": "10.38.0",
    "@styled-icons/boxicons-solid": "10.38.0",
    "@styled-icons/fa-brands": "10.38.0",
    "@styled-icons/material-outlined": "10.34.0",
    "@styled-icons/remix-fill": "10.18.0",
    "@styled-icons/remix-line": "10.18.0",
    "graphql": "15.8.0",
    "lodash.xor": "4.5.0",
    "next": "12.0.7",
    "next-pwa": "5.4.4",
    "polished": "4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "0.28.1",
    "styled-components": "5.3.3",
    "styled-media-query": "2.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/preset-typescript": "7.16.5",
    "@next/eslint-plugin-next": "12.0.7",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/react": "6.4.9",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.3",
    "@types/lodash.xor": "4.5.6",
    "@types/node": "17.0.2",
    "@types/react": "17.0.37",
    "@types/react-slick": "0.23.7",
    "@types/styled-components": "5.1.18",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "apollo": "2.33.9",
    "babel-loader": "8.2.3",
    "babel-plugin-styled-components": "2.0.2",
    "chalk": "5.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "jest-styled-components": "7.0.8",
    "lint-staged": "12.1.3",
    "plop": "3.0.5",
    "prettier": "2.5.1",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "resolutions": {
    "graphql": "15.8.0"
  }
}
