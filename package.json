{
  "name": "won-games-web",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "serve": "next start",
    "export": "next build && next export",
    "lint": "eslint src --max-warnings=0",
    "pretty": "yarn prettier src --write",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --maxWorkers=50% --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node src/utils/graphql/convertToNonNull.js",
    "schema:clear": "node src/utils/graphql/clearSchema.js",
    "codegen:generate": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/queries**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/types",
    "codegen": "yarn schema:download && yarn schema:convert && yarn codegen:generate && yarn schema:clear",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook -s ./public",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test --findRelatedTests --bail"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn eslint --fix",
      "yarn prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.5.5",
    "@styled-icons/boxicons-regular": "10.38.0",
    "@styled-icons/boxicons-solid": "10.38.0",
    "@styled-icons/fa-brands": "10.26.0",
    "@styled-icons/material-outlined": "10.28.0",
    "@styled-icons/remix-fill": "10.18.0",
    "@styled-icons/remix-line": "10.18.0",
    "graphql": "15.8.0",
    "next": "10.0.9",
    "next-pwa": "5.0.6",
    "polished": "4.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "0.28.1",
    "styled-components": "5.2.1",
    "styled-media-query": "2.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/preset-typescript": "7.13.0",
    "@storybook/addon-essentials": "6.0.26",
    "@storybook/react": "6.0.26",
    "@testing-library/jest-dom": "5.16.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.34",
    "@types/react": "17.0.3",
    "@types/react-slick": "0.23.4",
    "@types/styled-components": "5.1.8",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "apollo": "2.33.9",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.12.0",
    "chalk": "4.1.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.5.4",
    "plop": "2.7.4",
    "prettier": "2.2.1",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">=14"
  },
  "resolutions": {
    "graphql": "15.8.0"
  }
}
